assert(bool(null) == false)
assert(bool(0) == false)
assert(bool(1) == true)
assert(bool(0.0) == false)
assert(bool(1.0) == true)
assert(bool("") == false)
assert(bool("1") == true)
assert(bool({}) == false)
assert(bool({1: 1}) == true)

assert(int(null) == 0)
assert(int(true) == 1)
assert(int(false) == 0)
assert(int(0.0) == 0)
assert(int(0.5) == 0)
assert(int(0.9) == 0)
assert(int(1.0) == 1)
assert(int(1.5) == 1)
assert(int("1") == 1)
assert(int("123") == 123)

assert(float(null) == 0.0)
assert(float(true) == 1.0)
assert(float(false) == 0.0)
assert(float(0) == 0.0)
assert(float(1) == 1.0)
assert(float("1.0") == 1.0)
assert(float("1.23") == 1.23)

assert(str(null) == "null")
assert(str(true) == "true")
assert(str(false) == "false")
assert(str(123) == "123")
assert(str(0.123) == "0.123")
